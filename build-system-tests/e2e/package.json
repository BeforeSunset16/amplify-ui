{
  "name": "canary-cypress",
  "version": "0.0.1",
  "license": "MIT",
  "private": true,
  "main": "index.js",
  "scripts": {
    "clean": "rimraf node_modules",
    "cucumber": "./node_modules/.bin/cucumber-js --config detox/cucumber.json --tags @react-native",
    "dev": "TZ=UTC cypress open",
    "test:web": "TZ=UTC cypress run --spec 'features/**/*.feature'",
    "test:react-native": "TZ=UTC cypress run --spec 'features/**/*.feature'",
    "detox:build:android:debug": "detox build --configuration android.emulator.debug",
    "detox:e2e:android:debug": "DETOX_CONFIGURATION=android.emulator.debug yarn cucumber"
  },
  "cypress-cucumber-preprocessor": {
    "filterSpecs": true,
    "stepDefinitions": [
      "cypress/integration/ui/[filepath]/*.steps.ts",
      "cypress/integration/common/*.ts"
    ]
  },
  "author": "",
  "devDependencies": {
    "@badeball/cypress-cucumber-preprocessor": "^18.0.6",
    "@bahmutov/cypress-esbuild-preprocessor": "~2.2.0",
    "@testing-library/cypress": "^9.0.0",
    "@types/node": "^22.7.5",
    "cypress": "^12.17.3",
    "detox": "^20.13.1",
    "dotenv-safe": "^8.2.0",
    "rimraf": "^3.0.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3"
  },
  "resolutions": {
    "**/cypress-cucumber-preprocessor/**/semver": "6.3.1",
    "**/cypress-cucumber-preprocessor/**/core-js-compat/**/semver": "7.5.4",
    "@cypress/request": "^3.0.0",
    "tough-cookie": "4.1.3"
  }
}
